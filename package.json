{
  "name": "assistant-relay",
  "version": "1.0.0",
  "description": "A rendering server for the LED Desk sign.",
  "private": true,
  "scripts": {
    "start": "nodemon -e ts --exec babel-node src --extensions \".ts,.tsx\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "babel src --out-dir build --extensions \".ts,.tsx\"",
    "serve": "node build/index.js",
    "blah": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krconv/led-sign-server.git"
  },
  "author": "Kodey Converse",
  "license": "MIT",
  "homepage": "https://github.com/krconv/led-sign-server#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "proto-loader": "^0.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.44",
    "@types/passport": "^1.0.1",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-strategy": "^0.2.35",
    "nodemon": "^1.19.2",
    "typescript": "^3.6.3"
  }
}
